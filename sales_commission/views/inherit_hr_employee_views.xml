<odoo>
    <data>

        <record id="inherit_employee_form_view_commission" model="ir.ui.view">
            <field name="name">inherit.employee.form.view.commission</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[last()]" position="after">
                    <page string="Commission" name="commission">
                        <group>
                            <field name="commission_plan_id"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>